{
  "name": "vnc-plugin",
  "displayName": "VNC plugin",
  "version": "0.0.1",
  "description": "VNC plugin",
  "private": true,
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/rszwajko/vnc-plugin.git"
  },
  "scripts": {
    "prepare": "husky",
    "clean": "rm -rf dist",
    "build": "yarn clean && NODE_ENV=production yarn webpack",
    "build-dev": "yarn clean && yarn webpack",
    "start": "yarn webpack serve --progress",
    "start-console": "./start-console.sh",
    "lint": "yarn eslint src",
    "webpack": "node -r ts-node/register ./node_modules/.bin/webpack"
  },
  "dependencies": {
    "react": "17.0.2"
  },
  "devDependencies": {
    "@openshift-console/dynamic-plugin-sdk": "4.19.0-prerelease.2",
    "@openshift-console/dynamic-plugin-sdk-internal": "4.19.0-prerelease.2",
    "@openshift-console/dynamic-plugin-sdk-webpack": "4.19.0-prerelease.2",
    "@openshift-console/plugin-shared": "^0.0.3",
    "@kubevirt-ui/kubevirt-api": "^1.4.0",
    "@types/node": "^18.x",
    "@types/react": "^17.0.40",
    "@types/react-dom": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^8.33.0",
    "@typescript-eslint/parser": "^8.33.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.11.0",
    "eslint": "^8.57.1",
    "eslint-plugin-react": "^7.29.3",
    "eslint-plugin-react-hooks": "^4.3.0",
    "husky": "9.1.7",
    "lint-staged": "^13.2.2",
    "prettier": "^2.5.1",
    "style-loader": "^3.2.1",
    "ts-loader": "9.x",
    "ts-node": "^10.9.2",
    "typescript": "5.7.x",
    "webpack": "^5.75.0",
    "webpack-cli": "5.1.4",
    "webpack-dev-server": "^5.1.0",
    "yarn": "^1.22.18"
  },
  "engines": {
    "node": ">=18.20.5",
    "npm": "^9.5.0 || >=10.5.2"
  },
  "lint-staged": {
    "*.{js,ts,tsx,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "!(*.js|*.jsx|*.ts|*.tsx)": [
      "prettier --write --ignore-unknown"
    ]
  }
}
